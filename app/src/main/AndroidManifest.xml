        </activity>
        <activity
            android:name="net.alaindonesia.silectric.ElectronicListActivity"
            android:label=""
            android:parentActivityName="net.alaindonesia.silectric.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.alaindonesia.silectric.MainActivity" />
        </activity>
        <activity
            android:name="net.alaindonesia.silectric.ElectronicActivity"
            android:label=""
